    "keywords": [
        "progressbar",
        "uup"
    ],
    "homepage": "https://nowise.se/oss/uup/progressbar",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Anders Lövgren",
            "email": "andlov@nowise.se"
        },
        {
            "name": "Anders Lövgren",
            "email": "anders.lovgren@interactivesolutions.se"
        }
    ],
    "require": {
        "php": ">=7.4"
